{
    "name": "swc-plugin-obsidian",
    "version": "0.1.0",
    "description": "SWC plugin for Obsidian to be used with Vite or NestJS",
    "author": "Guy Carmeli",
    "main": "target/wasm32-wasi/release/swc_plugin_obsidian.wasm",
    "scripts": {
      "build": "RUSTFLAGS=\"-C debuginfo=2\" cargo build --release --target wasm32-wasi",
      "copyBinary": "mkdir -p demo/node_modules/.swc & cp target/wasm32-wasi/release/swc_plugin_obsidian.wasm demo/node_modules/.swc/",
      "cleanBuildAndCopy": "cargo clean && yarn build && yarn copyBinary",
      "test": "echo \"Error: test command not implemented yet\" && exit 1"
    },
    "dependencies": {
      "@swc/core": "^1.3.96"
    },
    "devDependencies": {
      "@types/node": "^20.8.10",
      "typescript": "^5.2.2",
      "jest": "^29.7.0",
      "@swc/jest": "^0.2.29",
      "@types/jest": "^29.5.8"
    },
    "files": [],
    "license": "ISC",
    "keywords": [
      "swc-plugin"
    ],
    "preferUnplugged": true
}
